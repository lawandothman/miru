{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NEXT_PUBLIC_SENTRY_RELEASE=$VERCEL_GITHUB_COMMIT_SHA SENTRY_DSN=$SENTRY_DSN next build && yarn run upload-sourcemaps",
    "postbuild": "next-sitemap",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "generate": "graphql-codegen --config codegen.yaml",
    "upload-sourcemaps": "sentry-cli releases --org $SENTRY_ORG --project $SENTRY_PROJECT files $VERCEL_GITHUB_COMMIT_SHA upload-sourcemaps ./build/ &&  sentry-cli releases --org $SENTRY_ORG --project $SENTRY_PROJECT files $VERCEL_GITHUB_COMMIT_SHA upload-sourcemaps ./src/ --ext .js --ext .ts --ext .tsx  --url-prefix '~/static/js/'"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@next-auth/neo4j-adapter": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@sentry/nextjs": "^7.28.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "neo4j-driver": "v4",
    "next": "13.1.1",
    "next-auth": "^4.18.7",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.43",
    "nextjs-progressbar": "^0.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-infinite-scroller": "^1.2.6",
    "react-outside-click-handler": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.11",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.10",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-outside-click-handler": "^1.3.1",
    "autoprefixer": "^10.4.7",
    "eslint-config-custom": "*",
    "eslint-config-next": "13.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.0",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.8.0"
  }
}
