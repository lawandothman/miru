{
  "name": "omni",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate-types": "graphql-codegen --config codegen.yaml",
    "build": "pnpm run generate-types && tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "script:seed": "ts-node src/scripts/seed.ts",
    "dev": "nodemon -e ts --exec \"(tsc || exit 1) && node dist/index.js\"",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@sentry/node": "^7.31.0",
    "@sentry/profiling-node": "^0.0.12",
    "@sentry/tracing": "^7.31.0",
    "@sentry/utils": "^8.26.0",
    "axios": "^1.7.4",
    "axios-retry": "^4.5.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "esm": "^3.2.25",
    "graphql": "^16.9.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mixpanel": "^0.17.0",
    "neo4j-driver": "^5.9.1",
    "pino": "^9.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^18.11.18",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4"
  }
}
