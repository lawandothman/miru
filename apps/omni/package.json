{
  "name": "omni",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yaml",
    "build": "npm run generate && tsc",
    "lint": "eslint src --ext .ts",
