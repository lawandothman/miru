{
  "name": "omni",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yaml",
    "compile": "npm run generate && tsc",
    "lint": "eslint src/*/**.ts",
    "lint:fix": "eslint src/*/**.ts --fix",
    "dev": "nodemon -e ts --exec \"(tsc || exit 1) && node dist/index.js\"",
    "start": "npm run compile && node ./dist/index.js"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "axios": "^1.2.1",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "esm": "^3.2.25",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "neo4j-driver": "^5.3.0",
    "workspace": "^0.0.1-preview.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.16",
    "eslint-config-custom": "*",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
