{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "generate-types": {
      "outputs": ["src/__generated__/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "start": {
      "outputs": []
    }
  }
}
