{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"lint": {},
		"typecheck": {},
		"db:generate": {
			"cache": false
		},
		"db:push": {
			"cache": false
		}
	}
}
